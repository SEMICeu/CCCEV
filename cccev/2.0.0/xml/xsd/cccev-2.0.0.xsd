	<xsd:element name="issueDate" type="cbc:DateType"/>
	<xsd:element name="issueTime" type="cbc:TimeType"/>
	<xsd:element name="valueOf" type="cbc:IDReferenceType"/>
	<xsd:element name="languageID" type="cbc:IDType"/>
	<xsd:element name="localeCode" type="cbc:CodeType"/>
	<xsd:element name="measureValue" type="cbc:MeasureType"/>
	<xsd:element name="numericValue" type="cbc:NumericType"/>
	<xsd:element name="quantityValue" type="cbc:QuantityType"/>
	<xsd:element name="evidenceTypeClassification" type="cbc:CodeType"/>
	<xsd:element name="statusCode" type="cbc:CodeType"/>
