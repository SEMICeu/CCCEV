<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns="https://data.europa.eu/semanticassets/ns/cv/cccev_v2.0.1#" 
	xmlns:dcat-ap="http://data.europa.eu/r5r/"
	xmlns:cac="https://data.europa.eu/semanticassets/ns/cv/common/cac_v2.0.1#"
	xmlns:cbc="https://data.europa.eu/semanticassets/ns/cv/common/cbc_v2.0.1#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	xmlns:foaf="http://xmlns.com/foaf/0.1/"
	targetNamespace="https://data.europa.eu/semanticassets/ns/cv/cccev_v2.0.1#" elementFormDefault="qualified"
	attributeFormDefault="unqualified" version="2.0.1">

	<xsd:import namespace="https://data.europa.eu/semanticassets/ns/cv/common/cac_v2.0.1#"
		schemaLocation="../../../../common/2.0.1/xml/xsdrt/CV-CommonAggregateComponents.xsd"/>
	<xsd:import namespace="https://data.europa.eu/semanticassets/ns/cv/common/cbc_v2.0.1#"
		schemaLocation="../../../../common/2.0.1/xml/xsdrt/CV-CommonBasicComponents.xsd"/>
	<xsd:import namespace="http://data.europa.eu/r5r/"
		schemaLocation="../../../../dcat-ap/2.0.1/xml/dcat-ap_semic-cbc.xsd"/>
	<xsd:import namespace="http://xmlns.com/foaf/0.1/"
		schemaLocation="../../../../foaf/2.0.1/xml/foaf_semic-cbc.xsd"/>
	<xsd:import namespace="http://www.w3.org/2002/07/owl#"
		schemaLocation="../../../../owl/2.0.1/xml/owl.xsd"/>

	<!-- Declaration of elements -->
	
	<!-- Data properties declarations -->
	<xsd:element name="amountValue" type="cbc:AmountType"/>
	<xsd:element name="associatedRoleType" type="cbc:CodeType"/>
	<xsd:element name="bias" type="cbc:NumericType"/>
	<xsd:element name="codeValue" type="cbc:CodeType"/>
	<xsd:element name="confidentialityLevelType" type="cbc:CodeType"/>
	<xsd:element name="conformsTo" type="cbc:IDReferenceType"/>
	<xsd:element name="constraintOn" type="cbc:IDReferenceType"/>
	<xsd:element name="representsConcept" type="cbc:IDReferenceType"/>
	<xsd:element name="customDataType" type="cbc:CodeType"/>
	<xsd:element name="customFunction" type="cbc:TextType"/>
	<xsd:element name="dataSetType" type="cbc:CodeType"/>
	<xsd:element name="dateValue" type="cbc:DateType"/>
	<xsd:element name="documentDescription" type="cbc:DescriptionType"/>
	<xsd:element name="documentType" type="cbc:TextType"/>
	<xsd:element name="documentURI" type="cbc:IDType"/>
	<xsd:element name="error" type="cbc:CodeType"/>
	<xsd:element name="identifierValue" type="cbc:IDType"/>
	<xsd:element name="indicatorValue" type="cbc:IndicatorType"/>
	<xsd:element name="inEvidenceTypeList" type="cbc:IDReferenceType"/>
	<xsd:element name="partOf" type="cbc:IDReferenceType"/>
	<xsd:element name="issueDate" type="cbc:DateType"/>
	<xsd:element name="issueTime" type="cbc:TimeType"/>
	<xsd:element name="valueOf" type="cbc:IDReferenceType"/>
	<xsd:element name="languageID" type="cbc:IDType"/>
	<xsd:element name="localeCode" type="cbc:CodeType"/>
	<xsd:element name="measureValue" type="cbc:MeasureType"/>
	<xsd:element name="numericValue" type="cbc:NumericType"/>
	<xsd:element name="quantityValue" type="cbc:QuantityType"/>
	<xsd:element name="evidenceTypeClassification" type="cbc:CodeType"/>
	<xsd:element name="statusCode" type="cbc:CodeType"/>
	<xsd:element name="supportsRequirement" type="cbc:IDReferenceType"/>		
	<xsd:element name="supportsConcept" type="cbc:IDReferenceType"/>
	<xsd:element name="textValue" type="cbc:TextType"/>
	<xsd:element name="timeValue" type="cbc:TimeType"/>
	<xsd:element name="uriValue" type="cbc:URIType"/>
	<xsd:element name="valueIDRef" type="cbc:IDType"/>
	<xsd:element name="versionID" type="cbc:IDType"/>
	<xsd:element name="weight" type="cbc:NumericType"/>
	<xsd:element name="weightingConsiderationDescription" type="cbc:DescriptionType"/>
	<xsd:element name="weightingType" type="cbc:CodeType"/>
	<xsd:element name="xPath" type="cbc:TextType"/>

	<!-- Object properties -->
	
	<xsd:element name="derivedFrom" type="ReferenceFrameworkType"/>
	<xsd:element name="concept" type="ConceptType"/>
	<xsd:element name="evidence" type="EvidenceType"/>
	<xsd:element name="evidenceType" type="EvidenceTypeType"/>
	<xsd:element name="evidenceTypeCombinationList" type="EvidenceTypeListType"/>
	<xsd:element name="constraint" type="ConstraintType"/>
	<xsd:element name="about" type="cbc:IDReferenceType"/>
	<xsd:element name="qualifiedRelation" type="dcat-ap:RelationshipType"/>
	<xsd:element name="requirement" type="RequirementType"/>	
	<xsd:element name="value" type="ValueType"/>
	<xsd:element name="periodValue" type="cac:PeriodType"/>
	<xsd:element name="validityPeriod" type="cac:PeriodType"/>		
	<xsd:element name="specifiedIn" type="ConceptType"/>

	<!-- Element definition -->

	<xsd:complexType name="ConceptType">
		<xsd:complexContent>
			<xsd:extension base="owl:ThingType">
				<xsd:sequence>
					<xsd:element ref="cbc:id" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cbc:name" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="cbc:description" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cbc:type" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="cbc:qName" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="constraint" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="value" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="concept" minOccurs="0" maxOccurs="unbounded"/>			
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ConstraintType">
		<xsd:complexContent>
			<xsd:extension base="RequirementType">
				<xsd:sequence>
					<xsd:element ref="constraintOn" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="CriterionType">
		<xsd:complexContent>
			<xsd:extension base="RequirementType">
				<xsd:sequence>
					<xsd:element ref="weight" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="weightingConsiderationDescription" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="weightingType" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="bias" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="DocumentReferenceType">
		<xsd:complexContent>
			<xsd:extension base="dcat-ap:DatasetType">
				<xsd:sequence>
					<xsd:element ref="cbc:binaryObject" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="associatedRoleType" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="statusCode" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="validityPeriod" minOccurs="0"/>			
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="EvidenceType">
		<xsd:complexContent>
			<xsd:extension base="dcat-ap:DatasetType">
				<xsd:sequence>
					<xsd:element ref="confidentialityLevelType" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="error" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="conformsTo" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="supportsRequirement" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="supportsConcept" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="about" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
				<xsd:attributeGroup ref="cac:ThingAttributes"/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="EvidenceTypeListType">
		<xsd:complexContent>
			<xsd:extension base="owl:ThingType">
				<xsd:sequence>
					<xsd:element ref="cbc:id" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="evidenceType" minOccurs="0" maxOccurs="unbounded"/>			
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="EvidenceTypeType">
		<xsd:complexContent>
			<xsd:extension base="dcat-ap:DatasetType">
				<xsd:sequence>
					<xsd:element ref="evidenceTypeClassification" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="dataSetType" minOccurs="0" maxOccurs="1"/>
					<xsd:element ref="specifiedIn" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="InformationRequirementType">
		<xsd:complexContent>
			<xsd:extension base="RequirementType">
				<xsd:sequence/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ReferenceFrameworkType">
		<xsd:complexContent>
			<xsd:extension base="dcat-ap:DatasetType">
				<xsd:sequence/>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="RequirementType" abstract="false">
		<xsd:complexContent>
			<xsd:extension base="owl:ThingType">
				<xsd:sequence>
					<xsd:element ref="cbc:id" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cbc:name" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cbc:description" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="cbc:type" minOccurs="0"/>
					<xsd:element ref="requirement" minOccurs="0" maxOccurs="unbounded"/>			
					<xsd:element ref="concept" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="evidence" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="evidenceTypeCombinationList" minOccurs="0" maxOccurs="unbounded"/>			
					<xsd:element ref="partOf" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="qualifiedRelation" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="derivedFrom" minOccurs="0" maxOccurs="unbounded"/>						
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>

	<xsd:complexType name="ValueType">
		<xsd:complexContent>
			<xsd:extension base="owl:ThingType">
				<xsd:sequence>
					<xsd:element ref="amountValue" minOccurs="0"/>
					<xsd:element ref="codeValue" minOccurs="0"/>
					<xsd:element ref="dateValue" minOccurs="0"/>
					<xsd:element ref="identifierValue" minOccurs="0"/>
					<xsd:element ref="indicatorValue" minOccurs="0"/>
					<xsd:element ref="measureValue" minOccurs="0"/>
					<xsd:element ref="numericValue" minOccurs="0"/>
					<xsd:element ref="quantityValue" minOccurs="0"/>
					<xsd:element ref="textValue" minOccurs="0" maxOccurs="unbounded"/>
					<xsd:element ref="timeValue" minOccurs="0"/>
					<xsd:element ref="uriValue" minOccurs="0"/>
					<xsd:element ref="customDataType" minOccurs="0"/>
					<xsd:element ref="customFunction" minOccurs="0"/>
					<xsd:element ref="error" minOccurs="0"/>
					<xsd:element ref="periodValue" minOccurs="0"/>			
					<xsd:element ref="valueOf" minOccurs="0"/>
					<xsd:element ref="value" minOccurs="0" maxOccurs="unbounded"/>
				</xsd:sequence>
			</xsd:extension>
		</xsd:complexContent>
	</xsd:complexType>
</xsd:schema>
